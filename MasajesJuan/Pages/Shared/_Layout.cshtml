<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <title>Salon Juan</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="stylesheet" href="css/estilosSASS.css">
    <link rel="stylesheet" href="css/efectos.css">
</head>

<body onload="myFunction()">
    <header>
        <div id="logo">
            <a asp-area="" asp-page="/Index"><img src="images/Logo.png" /></a>
        </div>
        <div>
            <nav id="site-nav">
                <ul class="menu">
                    <li><a asp-area="" asp-page="/Salon">Servicios</a></li>
                    <li><a asp-area="" asp-page="/Privacy">Privacy</a></li>
                    <li><a href="#">Contacto</a></li>
                    <li><a href="#">Lao-Tse</a></li>
                </ul>
            </nav>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - WebApplication1 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script src="~/js/app.js"></script>
    <script src="~/js/file.js"></script>
</body>
</html>
